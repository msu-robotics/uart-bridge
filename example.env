# HTTP сервер
HTTP_HOST=0.0.0.0
HTTP_PORT=8000

# UART конфигурация
UART_PORT=/dev/ttyUSB0
# UART_PORT=COM3  # для Windows
UART_BAUDRATE=115200
UART_BYTESIZE=8
UART_STOPBITS=1
UART_PARITY=N
UART_TIMEOUT=1.0
UART_WRITE_TIMEOUT=1.0

# WebSocket
WS_PING_INTERVAL=30
WS_MAX_SIZE=104857600
WS_PING_TIMEOUT=10

# Логирование
LOG_LEVEL=INFO